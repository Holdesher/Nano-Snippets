{
	"name": "nanosnippets",
	"publisher": "kah3vich",
	"displayName": "Nano-Snippets",
	"description": "Snippets for various technologies and languages, taking into account all modern trends and rules.",
	"repository": {
		"type": "git",
		"url": "https://github.com/kah3vich/Nano-Snippets"
	},
	"version": "0.0.14",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Snippets"
	],
	"preview": false,
	"galleryBanner": {
		"color": "#ffffff",
		"theme": "light"
	},
	"main": "./out/src/extension.js",
	"browser": "./out/src/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/html.json"
			},
			{
				"language": "jade",
				"path": "./snippets/pug.json"
			},
			{
				"language": "scss",
				"path": "./snippets/scss.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/javascript.json"
			}
		],
		"commands": [
			{
				"command": "nano-snippets",
				"title": "Nano Snippets"
			}
		]
	},
	"activationEvents": [
		"onCommand:nano-snippets"
	],
	"icon": "assets/logo.png",
	"license": "GPL-3.0-only",
	"author": {
		"name": "kah3vich",
		"url": "https://github.com/kah3vich"
	},
	"bugs": {
		"url": "https://github.com/kah3vich/Nano-Snippets/issues"
	},
	"keywords": [
		"nano",
		"snippets",
		"nanosnippets",
		"snippet",
		"snippets",
		"react",
		"html",
		"scss",
		"style",
		"css",
		"pug",
		"ts",
		"js",
		"typescript",
		"javascript",
		"sass",
		"jquery"
	],
	"homepage": "https://github.com/kah3vich/Nano-Snippets/readme.md",
	"scripts": {
		"build": "vsce package",
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"test": "node ./node_modules/@vscode/test-electron/out/runTest.js"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.32",
		"@types/node": "^6.0.40",
		"@types/vscode": "^1.63.0",
		"@vscode/test-electron": "^1.6.1",
		"mocha": "^8.0.1",
		"typescript": "^4.0.0"
	}
}
